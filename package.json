{
  "name": "customer-dashboard",
  "version": "0.0.0",
  "private": true,
  "description": "Kaboodle Solutions Ltd Customer Dashboard UI",
  "license": "Unlicense",
  "contributors": [
    "Sunny Lum <sunny.lum@kaboodle.co.uk>",
    "David Henderson <david.henderson@kaboodle.co.uk>",
    "Tom Wilkinson <tom.wilkinson@kaboodle.co.uk>",
    "Josh McEwen <josh.mcewen@kaboodle.co.uk>",
    "Przemek Lewtak <przemek.lewtak@kaboodle.co.uk>"
  ],
  "main": "./src/index.js",
  "scripts": {
    "build": "NODE_ENV=production node ./node_modules/.bin/webpack -p --config webpack.prod.babel.js --progress --colors",
    "build:dev": "webpack --config webpack.dev.babel.js --display-error-details --progress --colors",
    "format": "sure --question \"This is a potentially destructive operation. Continue? [yN]\" && prettier --write 'src/**/*.js'",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint --max-warnings=0 \"src/**/*.{s{a,c},c}ss\"",
    "lint:js": "prettier --check 'src/**/*.js' && eslint --max-warnings=0 src",
    "test": "jest",
    "watch": "npm run build:dev -- --watch"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "^.+\\.styles\\.js$"
    ],
    "globalSetup": "./src/setupTestsGlobal.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "modulePaths": [
      "./src"
    ],
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@kaboodle-solutions/design-system": "^1.7.1",
    "@nivo/pie": "^0.61.1",
    "@sentry/browser": "^5.4.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.5",
    "color": "^3.1.2",
    "connected-react-router": "^6.3.2",
    "date-fns": "^1.30.1",
    "formik": "^2.1.3",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "lodash.noop": "^3.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-stripe-elements": "^6.0.1",
    "react-transition-group": "^2.9.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1",
    "redux-sentry-middleware": "^0.0.15",
    "reselect": "^4.0.0",
    "sanitize.css": "^8.0.0",
    "validator": "^10.11.0",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@kaboodle-solutions/frontend-config": "^1.0.0",
    "@sentry/webpack-plugin": "^1.6.2",
    "autoprefixer": "^9.4.8",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "debug": "^4.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-redux": "^3.0.1",
    "jest": "^24.1.0",
    "jest-date-mock": "^1.0.7",
    "jest-junit": "^6.4.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.7.1",
    "react-testing-library": "^6.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-saga-test-plan": "^4.0.0-beta.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "sure": "0.0.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.4.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
